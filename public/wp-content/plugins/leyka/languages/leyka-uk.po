"ЄДРПОУ #UA_ERDPOU#\n"
"#LEGAL_ADDRESS#\n"
"#SITE_URL# #ADMIN_EMAIL#\n"
"\n"
"#BANK_NAME#\n"
"р/р #BANK_ACCOUNT#\n"
"\n"
"#LEGAL_FACE_POSITION# #LEGAL_FACE#"

